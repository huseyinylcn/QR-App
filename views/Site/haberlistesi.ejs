<%- include('../Particals/header.ejs') %>
<link rel="stylesheet" href="/css/kopyalar.css">
<div class="container">
    <div class="row">

        <% veri.forEach(function(item) { %>

            <div class="hbBox col-xl-3 col-lg-3 col-md-4 col-sm-12">
                <a href="/haberekle/duzenle/<%= item.id %>">
                <div class="kose">
                    <div class="resimkutu">
                        <img class="hbBoxImg"
                            src="<%= item.kapakfotograf %>"
                            alt="">
                    </div>
                    <p><%= item.baslik %></p>
                </div>
            </a>
            </div>

           
        <% }); %>



    



    </div>
</div>
<script src="/js/kopyalar.js"></script>
    <%- include('../Particals/footer.ejs') %>